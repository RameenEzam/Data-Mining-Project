      "cell_type": "code",
      "source": [
        "# scipy is not necessary here since we have already scaled our data but provides an alternative way to scale data\n",
        "from scipy import stats\n",
        "\n",
        "z = np.abs(stats.zscore(df._get_numeric_data()))\n",
        "print(z)\n",
        "df= df[(z < 3).all(axis = 1 )]\n",
        "print(df.shape)"
      ],
      "metadata": {
        "colab": {
